local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "klain hub",
   LoadingTitle = "Загрузка...",
   LoadingSubtitle = "by klainkuni",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "klainhub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   Theme = "Bloom"
})


local MainTab = Window:CreateTab("Main", 4483362458)

MainTab:CreateButton({
   Name = "Infinite Yield",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end
})

MainTab:CreateButton({
   Name = "Unload",
   Callback = function()
      Rayfield:Destroy()
   end
})


local PlayerTab = Window:CreateTab("Player", 4483362458)

PlayerTab:CreateSlider({
   Name = "WalkSpeed",
   Range = {16, 1500},
   Increment = 1,
   CurrentValue = 16,
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

PlayerTab:CreateSlider({
   Name = "JumpPower",
   Range = {50, 1500},
   Increment = 1,
   CurrentValue = 50,
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
   end,
})

local ClickTpEnabled = false
PlayerTab:CreateToggle({
   Name = "Click TP (Ctrl + ЛКМ)",
   CurrentValue = false,
   Callback = function(Value)
      ClickTpEnabled = Value
   end,
})

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
   if ClickTpEnabled and input.UserInputType == Enum.UserInputType.MouseButton1 and game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
      local mouse = game.Players.LocalPlayer:GetMouse()
      if mouse then
         local char = game.Players.LocalPlayer.Character
         if char and char:FindFirstChild("HumanoidRootPart") then
            char:MoveTo(mouse.Hit.p + Vector3.new(0, 2, 0))
         end
      end
   end
end)


local NoclipEnabled = false
game:GetService("RunService").Stepped:Connect(function()
   if NoclipEnabled then
      for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
         if part:IsA("BasePart") then
            part.CanCollide = false
         end
      end
   end
end)

PlayerTab:CreateToggle({
   Name = "Noclip",
   CurrentValue = false,
   Callback = function(Value)
      NoclipEnabled = Value
   end,
})


PlayerTab:CreateButton({
   Name = "Fly GUI V3",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
   end,
})


PlayerTab:CreateInput({
   Name = "Телепорт к игроку",
   PlaceholderText = "Введи ник игрока",
   RemoveTextAfterFocusLost = false,
   Callback = function(playerName)
      local target = game.Players:FindFirstChild(playerName)
      local localPlayer = game.Players.LocalPlayer
      if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
         localPlayer.Character:MoveTo(target.Character.HumanoidRootPart.Position + Vector3.new(0, 3, 0))
      end
   end,
})


local GardenTab = Window:CreateTab("Grow a Garden", 4483362458)
GardenTab:CreateButton({
   Name = "Thao Hub",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/game/refs/heads/main/TbaoHubGrowGarden"))()
   end
})


local MM2Tab = Window:CreateTab("MM2", 4483362458)

MM2Tab:CreateButton({
   Name = "SnapSanixHUB",
   Callback = function()
      loadstring(game:HttpGet('https://raw.githubusercontent.com/Roman34296589/SnapSanixHUB/refs/heads/main/SnapSanixHUB.lua'))()
   end,
})

MM2Tab:CreateButton({
   Name = "Xhub",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Au0yX/Community/main/XhubMM2"))()
   end,
})

MM2Tab:CreateButton({
   Name = "hook",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/kiciahook/kiciahook/refs/heads/main/loader.lua"))()
   end,
})


local Credits = Window:CreateTab("Credits", 4483362458)
Credits:CreateLabel("Created by klainkuni")


